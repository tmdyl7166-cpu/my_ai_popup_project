{
  "manifest_version": 2,
  "name": "iRoop Face Swapper",
  "version": "1.0",
  "description": "Click to swap faces on the current page using the local Python app.",
  "permissions": [
    "activeTab",
    "http://127.0.0.1:5000/*"
  ],
  "icons": {
    "48": "icon.png",
    "96": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "iRoop Swap"
  }
}